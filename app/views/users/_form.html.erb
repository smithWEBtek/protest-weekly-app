<%= form_for :user, url: users_path do |f| %>
	

	<%= f.label :name %><br>
	<%= f.text_field :name  %> 


	<%= f.label :email %><br>
	<%= f.email_field :email %>

	<%= f.label :password %><br>
	<%= f.password_field :password %>

	<%= f.label :cell_phone %><br>
	<%= f.telephone_field :cell_phone %>


	<%= f.submit "New User" %>

<% end %>
			