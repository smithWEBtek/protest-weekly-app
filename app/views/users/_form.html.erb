<%= form_for :user, url: users_path do |f| %>
	
<div class="form-group">
	<%= f.label :name %><br>
	<%= f.text_field :name, class: "form-control"  %></div> 

<div class="form-group">
	<%= f.label :email %><br>
	<%= f.email_field :email, class: "form-control" %></div>

<div class="form-group">
	<%= f.label :password %><br>
	<%= f.password_field :password, class: "form-control" %></div>

<div class="form-group">
	<%= f.label :cell_phone %><br>
	<%= f.telephone_field :cell_phone, class: "form-control" %></div>


<div class="actions">
	<%= f.submit class: "btn btn-primary" %>
</div>

<% end %>
			