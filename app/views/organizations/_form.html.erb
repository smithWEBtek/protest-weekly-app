<%= form_for @organization do |f| %>

<div class="form-group">
	<%= f.label :name %><br>
	<%= f.text_field :name, class: "form-control" %> </div>

<div class="form-group">
	<%= f.label :contact_info %><br>
	<%= f.text_field :contact_info, class: "form-control" %> </div>

	<div class="actions">
	<%= f.submit class: "btn btn-primary" %>
	</div>
	
<% end %>
			
	