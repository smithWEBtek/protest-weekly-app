<%= form_for @venue do |f| %>

	<div class="form-group">
	<%= f.label :name %><br>
	<%= f.text_field :name, class: "form-control" %> </div>

	<div class="form-group">
	<%= f.label :street_address %><br>
	<%= f.text_field :street_address, class: "form-control" %> </div>

	<div class="form-group">
	<%= f.label :city %><br>
	<%= f.text_field :city, class: "form-control" %> </div>

	<div class="form-group">
	<%= f.label :state %><br>
	<%= f.text_field :state, class: "form-control" %> </div>

	<div class="actions">
	<%= f.submit class: "btn btn-primary" %>
</div>

<% end %>